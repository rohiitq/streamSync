{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "StreamSync",
  "description": "This is a Chrome extension built with React and TypeScript",
  "action": {
    "default_popup": "./index.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  }
}